{
    "dest": "img/favicon/",
    "iconsPath": "img/favicon/",
    "markupFile": "favicon-data.json",
    "masterPicture": "src/img/favicon/favicon.svg",
    "design": {
        "ios": {
            "pictureAspect": "backgroundAndMargin",
            "backgroundColor": "#fff",
            "margin": "14%",
            "appName": "Vitality"
        },
        "desktopBrowser": { },
        "windows": {
            "pictureAspect": "whiteSilhouette",
            "backgroundColor": "#f41c5e",
            "onConflict": "override",
            "appName": "Vitality"
        },
        "androidChrome": {
            "pictureAspect": "backgroundAndMargin",
            "margin": "17%",
            "backgroundColor": "#fff",
            "themeColor": "#fff",
            "manifest": {
                "name": "Vitality",
                "display": "browser",
                "orientation": "notSet",
                "onConflict": "override",
                "declared": true
            }
        },
        "safariPinnedTab": {
            "pictureAspect": "silhouette",
            "themeColor": "#f41c5e"
        },
        "openGraph": {
            "pictureAspect": "backgroundAndMargin",
            "backgroundColor": "#fff",
            "margin": "30%",
            "ratio": "1.91:1",
            "siteUrl": "https://www.vitality.co.uk/"
        }
    },
    "settings": {
        "compression": 1,
        "scalingAlgorithm": "Mitchell",
        "errorOnImageTooSmall": false
    }
}
