<body>
    <div class="page">
        <div class="cookie-message">
            <div class="grid">
                <div class="grid-col-9-12">
                    <p>
                        Vitality.co.uk uses <a href="#">cookies</a> (small text files)
                        to store information on your computer to give you the best experience.
                    </p>
                </div>

                <div class="grid-col-3-12">
                    <a class="box-button box-button--light box-button--rounded">Close</a>
                </div>
            </div>
        </div>

        <header class="page-header">
            <!-- top-bar mobile -->
            <div class="top-bar top-bar--small">
                <div class="top-bar__home">
                    <a href="#">
                        <img src="/src/img/examples/vitality-logo.svg" alt="Vitality" class="top-bar__logo">
                    </a>
                </div>

                <ul class="utility-nav--small">
                    <li class="utility-nav__item utility-nav__item--search js-search-toggle">
                        <div class="utility-nav__inner">
                            <a href="search.html">
                                <img src="/src/img/examples/search.svg" alt="Search">
                                <span>Search</span>
                            </a>
                        </div>
                    </li>

                    <li class="utility-nav__item utility-nav__item--log-in js-log-in-toggle">
                        <div class="utility-nav__inner ">
                            <img src="/src/img/examples/log-in.svg" alt="Log in">
                            <span>Log In</span>
                        </div>
                    </li>

                    <li class="utility-nav__item utility-nav__item--menu js-main-menu-toggle">
                        <div class="utility-nav__inner">
                            <div class="burger-menu"><span></span></div>
                            <span>Menu</span>
                        </div>
                    </li>
                </ul>
            </div>
            <!-- end top-bar mobile -->
            <!-- top-bar desktop - contains site nav -->
            <div class="top-bar top-bar--large">
                <nav role="navigation">
                    <ul class="site-nav">
                        <li class="site-nav__item is-active">
                            <a href="#">Personal</a>
                        </li>

                        <li class="site-nav__item">
                            <a href="#">Business</a>
                        </li>

                        <li class="site-nav__item">
                            <a href="#">Advisers</a>
                        </li>

                        <li class="site-nav__item">
                            <a href="#">Healthcare Providers</a>
                        </li>
                    </ul>
                </nav>

                <ul class="utility-nav">
                    <li class="utility-nav__item utility-nav__item--search js-search-toggle">
                        <a href="search.html">
                            <img src="/src/img/examples/search.svg" alt="Search">
                            Search
                        </a>
                    </li>

                    <li class="utility-nav__item utility-nav__item--log-in js-log-in-toggle">
                        <a href="#">Log In</a>

                        <!-- log in panel desktop -->
                        <div class="log-in log-in--large">
                            <div class="log-in__content">
                                <div class="log-in__links">
                                    <a href="#" class="box-button box-button--rounded box-button--secondary box-button--full-width">
                                        Member Log-in
                                    </a>
                                    <a href="#" class="box-button box-button--rounded box-button--full-width">
                                        Adviser Log-in
                                    </a>

                                    <a href="#" class="box-button box-button--rounded box-button--full-width">
                                        Employer Log-in
                                    </a>

                                    <a href="#" class="box-button box-button--rounded box-button--full-width">
                                        Healthcare Practitioner Log-in
                                    </a>
                                </div>
                            </div>
                        </div>
                        <!-- end log in panel desktop-->
                    </li>

                    <li class="utility-nav__item">
                        <a href="#">Contact us</a>
                    </li>

                    <li class="utility-nav__item utility-nav__item--social">
                        <ul class="social-icons-list">
                            <li class="social-icon">
                                <a href="https://www.facebook.com/VitalityUK" target="_blank" class="svg-sprite svg-facebook"></a>
                            </li>

                            <li class="social-icon">
                                <a href="https://twitter.com/Vitality_UK" target="_blank" class="svg-sprite svg-twitter"></a>
                            </li>

                            <li class="social-icon">
                                <a href="https://www.instagram.com/vitality_uk/?hl=en" target="_blank" class="svg-sprite svg-instagram"></a>
                            </li>

