<configuration xmlns:patch="http://www.sitecore.net/xmlconfig/" xmlns:set="http://www.sitecore.net/xmlconfig/set/">
    <sitecore>
        <scheduling>
            <!-- An agent that processes sitemap generation  -->
            <!-- octopus to only deploy this to CDs -->
            <agent type="Vitality.Website.SC.Agents.SitemapGenerator" method="Run" interval="00:00:30" name="SitemapGenerator">
                <param desc="database">web</param>
            </agent>
        </scheduling>
    </sitecore>
</configuration>
