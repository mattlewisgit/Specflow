<configuration xmlns:patch="http://www.sitecore.net/xmlconfig/">
  <sitecore>
    <pipelines>
      <initialize>
        <processor type="Vitality.Website.App_Start.Ioc, Vitality.Website"/>
      </initialize>
      <mvc.exception>
        <processor type="Vitality.SC.Pipelines.MvcEvents.Exception.GeneralExceptionProcessor, Vitality.SC" 
                   patch:instead="*[@type='Sitecore.Mvc.Pipelines.MvcEvents.Exception.ShowAspNetErrorMessage, Sitecore.Mvc']"/>
      </mvc.exception>
    </pipelines>
  </sitecore>
</configuration>