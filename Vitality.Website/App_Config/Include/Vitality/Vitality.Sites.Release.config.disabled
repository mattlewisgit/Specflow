<configuration xmlns:patch="http://www.sitecore.net/xmlconfig/" xmlns:set="http://www.sitecore.net/xmlconfig/set/">
  <sitecore>
    <sites>
      <site name="presales"
            set:hostName="#{Vitality.Website.PresalesHostName}"
            set:cacheHtml="true"
            set:database="#{Vitality.Website.Database}"
            set:scheme="#{Vitality.Website.Protocol}"
            set:htmlCacheSize="100MB"
            set:cacheRenderingParameters="true"
            set:renderingParametersCacheSize="20MB" />
