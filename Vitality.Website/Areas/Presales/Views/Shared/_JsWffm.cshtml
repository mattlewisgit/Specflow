@using Sitecore.Forms.Mvc
@{
    var scriptsBaseUrlUrlContent = Url.Content(Constants.ScriptsBaseUrl);
    var scriptsBaseUrlUrlContentDecoded = HttpUtility.UrlDecode(scriptsBaseUrlUrlContent);
}
<script type="text/javascript" src="//cdnjs.cloudflare.com/ajax/libs/jqueryui/1.11.4/jquery-ui.min.js"></script>
<script>if (!($.datepicker)) cdnizerLoad("/bower_components/jqueryui/jquery-ui.min.js");</script>
<script type="text/javascript" src="//cdnjs.cloudflare.com/ajax/libs/jquery-validate/1.13.1/jquery.validate.min.js"></script>
<script>if (!($.validator)) cdnizerLoad("@(scriptsBaseUrlUrlContentDecoded + "libs/jquery/jquery.validate.min.js")");</script>
<script type="text/javascript" src="//cdnjs.cloudflare.com/ajax/libs/jquery-validation-unobtrusive/3.2.6/jquery.validate.unobtrusive.min.js"></script>
<script>if (!($.validator.unobtrusive)) cdnizerLoad("@(scriptsBaseUrlUrlContentDecoded + "libs/jquery/jquery.validate.unobtrusive.min.js"))");</script>
<script type="text/javascript" src="@(scriptsBaseUrlUrlContent +"wffm.min.js")"></script>

@*
    The WFFM main file is swapped out for a customised Vitality version,
    which is bundled as part of the minified presales script.
    <script type="text/javascript" src="@(scriptsBaseUrlUrlContent + "main.min.js")"></script>
*@
